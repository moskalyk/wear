#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mwm/Projects/sequence/wear/relayer/node_modules/.pnpm/ipfs-car@0.7.0_node-fetch@3.3.1/node_modules/ipfs-car/dist/cjs/cli/node_modules:/home/mwm/Projects/sequence/wear/relayer/node_modules/.pnpm/ipfs-car@0.7.0_node-fetch@3.3.1/node_modules/ipfs-car/dist/cjs/node_modules:/home/mwm/Projects/sequence/wear/relayer/node_modules/.pnpm/ipfs-car@0.7.0_node-fetch@3.3.1/node_modules/ipfs-car/dist/node_modules:/home/mwm/Projects/sequence/wear/relayer/node_modules/.pnpm/ipfs-car@0.7.0_node-fetch@3.3.1/node_modules/ipfs-car/node_modules:/home/mwm/Projects/sequence/wear/relayer/node_modules/.pnpm/ipfs-car@0.7.0_node-fetch@3.3.1/node_modules:/home/mwm/Projects/sequence/wear/relayer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mwm/Projects/sequence/wear/relayer/node_modules/.pnpm/ipfs-car@0.7.0_node-fetch@3.3.1/node_modules/ipfs-car/dist/cjs/cli/node_modules:/home/mwm/Projects/sequence/wear/relayer/node_modules/.pnpm/ipfs-car@0.7.0_node-fetch@3.3.1/node_modules/ipfs-car/dist/cjs/node_modules:/home/mwm/Projects/sequence/wear/relayer/node_modules/.pnpm/ipfs-car@0.7.0_node-fetch@3.3.1/node_modules/ipfs-car/dist/node_modules:/home/mwm/Projects/sequence/wear/relayer/node_modules/.pnpm/ipfs-car@0.7.0_node-fetch@3.3.1/node_modules/ipfs-car/node_modules:/home/mwm/Projects/sequence/wear/relayer/node_modules/.pnpm/ipfs-car@0.7.0_node-fetch@3.3.1/node_modules:/home/mwm/Projects/sequence/wear/relayer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ipfs-car/dist/cjs/cli/cli.js" "$@"
else
  exec node  "$basedir/../ipfs-car/dist/cjs/cli/cli.js" "$@"
fi
